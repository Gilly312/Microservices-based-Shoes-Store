<!DOCTYPE html>
<html>

<head>
	<title>TDT Sneaker</title>
	<link href="stylesheet/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
	<!--jQuery(necessary for Bootstrap's JavaScript plugins)-->
	<script src="javascripts/jquery-1.11.0.min.js"></script>
	<!--Custom-Theme-files-->
	<!--theme-style-->
	<link href="stylesheet/style.css" rel="stylesheet" type="text/css" media="all" />
	<!--//theme-style-->
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="keywords" content="Luxury Watches Responsive web template, Bootstrap Web Templates, Flat Web Templates, Andriod Compatible web template, 
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
	<script
		type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
	<!--start-menu-->
	<script src="/javascripts/simpleCart.min.js"> </script>
	<link href="/stylesheet/memenu.css" rel="stylesheet" type="text/css" media="all" />
	<script type="text/javascript" src="/javascripts/memenu.js"></script>
	<script>$(document).ready(function () { $(".memenu").memenu(); });</script>
	<!--dropdown-->
	<script src="/javascripts/jquery.easydropdown.js"></script>
</head>

<body>
	<!--top-header-->
	<div class="top-header">
		<div class="container">
			<div class="top-header-main">
				<div class="col-md-6 top-header-left">
					<div>
						<a href="/"><img
								src="https://cdn.haitrieu.com/wp-content/uploads/2021/09/Logo-DH-Ton-Duc-Thang-TDT.png"
								alt="" style="width:10%"></a>
					</div>
					<% if(locals.user) { %>
						<div>
							<a href="/logout">
								<%= user.name %> Log out
							</a>
						</div>
						<% } %>
				</div>
				<div class="col-md-6 top-header-left">
					<div class="cart box_1">
						<a href="/cart">
							<div class="total">
								<span>
									<% if (locals.totalPrice) { %>
										<%= locals.totalPrice %>
											<% } else { %>
												0
												<% } %>\
								</span>
							</div>
							<img src="images/cart-1.png" alt="" />
						</a>
						<% if (locals.cart) { %>
							<p><a href="javascript:;" class="simpleCart_empty">
									<%= cart %> Items
								</a></p>
							<% }else { %>
								<p><a href="javascript:;" class="simpleCart_empty">Empty Cart</a></p>
								<% } %>
									<div class="clearfix"> </div>
					</div>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>
	<!--top-header-->
	<!--start-logo-->
	<div class="logo">
		<a href="/">
			<h1>TDT Sneaker</h1>
		</a>
	</div>
	<!--start-logo-->
	<!--bottom-header-->
	<div class="header-bottom">
		<div class="container">
			<div class="header">
				<div class="col-md-9 header-left">
					<div class="top-nav">
						<ul class="memenu skyblue">
							<li class="active"><a href="/">Home</a></li>
							<li class="grid"><a href="/product/Men">Men</a>
								<div class="mepanel">
									<div class="row">
										<div class="col1 me-one">
											<h4>Regular</h4>
											<ul>
												<li><a href="/product/filter/Male/Sport">Sport Watch</a></li>
												<li><a href="/product/filter/Male/Digital">Digital Watch</a></li>
												<li><a href="/product/filter/Male/Smart">Smart Watch</a></li>
												<li><a href="/product/filter/Male/Luxury">Luxury Watch</a></li>
											</ul>
										</div>
										<div class="col1 me-one">
											<h4>Luxury</h4>
											<ul>
												<li><a href="/product/brand/rolex">Rolex</a></li>
												<li><a href="/product/brand/julius">Fastrack</a></li>
												<li><a href="/product/brand/casio">Casio</a></li>
												<li><a href="/product/brand/timex">Timex</a></li>
											</ul>
										</div>
									</div>
								</div>
							</li>
							<li class="grid"><a href="/product/Women">Women</a>
								<div class="mepanel">
									<div class="row">
										<div class="col1 me-one">
											<h4>Regular</h4>
											<ul>
												<li><a href="/product/filter/Female/Sport">Sport Watch</a></li>
												<li><a href="/product/filter/Female/Digital">Digital Watch</a></li>
												<li><a href="/product/filter/Female/Smart">Smart Watch</a></li>
												<li><a href="/product/filter/Female/Luxury">Luxury Watch</a></li>
											</ul>
										</div>
										<div class="col1 me-one">
											<h4>Luxury</h4>
											<ul>
												<li><a href="/product/brand/rolex">Rolex</a></li>
												<li><a href="/product/brand/julius">Fastrack</a></li>
												<li><a href="/product/brand/casio">Casio</a></li>
												<li><a href="/product/brand/timex">Timex</a></li>
											</ul>
										</div>
									</div>
								</div>
							</li>
						</ul>
					</div>
					<div class="clearfix"> </div>
				</div>
				<div class="clearfix"> </div>
			</div>
		</div>
	</div>
	<!--bottom-header-->
	<!--banner-starts-->
	<div class="bnr" id="home">
		<div id="top" class="callbacks_container">
			<ul class="rslides" id="slider4">
				<li>
					<img style="object-fit: contain;"
						src="https://c4.wallpaperflare.com/wallpaper/722/397/228/michael-jordan-chicago-bulls-nike-basketball-wallpaper-preview.jpg"
						alt="" />
				</li>
				<li>
					<img style="object-fit: contain"
						src="https://c4.wallpaperflare.com/wallpaper/722/397/228/michael-jordan-chicago-bulls-nike-basketball-wallpaper-preview.jpg"
						alt="" />
				</li>
			</ul>
		</div>
		<div class="clearfix"> </div>
	</div>
	<!--banner-ends-->
	<!--Slider-Starts-Here-->
	<script src="javascripts/responsiveslides.min.js"></script>
	<script>
		// You can also use "$(window).load(function() {"
		$(function () {
			// Slideshow 4
			$("#slider4").responsiveSlides({
				auto: true,
				pager: true,
				nav: true,
				speed: 500,
				namespace: "callbacks",
				before: function () {
					$('.events').append("<li>before event fired.</li>");
				},
				after: function () {
					$('.events').append("<li>after event fired.</li>");
				}
			});
		});
	</script>
	<!--End-slider-script-->
	<!--about-starts-->
	<div class="about">
		<div class="container">
			<div class="about-top grid-1">
				<div class="col-md-4 about-left">
					<figure class="effect-bubba">
						<img class="img-responsive"
							src="https://images.stockx.com/images/Air-Jordan-3-Retro-DJ-Khaled-Grateful-Product.jpg?fit=fill&bg=FFFFFF&w=480&h=320&fm=webp&auto=compress&dpr=1&trim=color&updated_at=1607651038&q=80"
							alt="" />
						<figcaption>
							<h2>Sneaker for Men</h2>
							<p>Luxury define your lifestyle</p>
						</figcaption>
					</figure>
				</div>
				<div class="col-md-4 about-left">
					<figure class="effect-bubba">
						<img class="img-responsive"
							src="https://images.stockx.com/images/Air-Jordan-3-Retro-Solefly-Lotto.png?fit=fill&bg=FFFFFF&w=480&h=320&fm=webp&auto=compress&dpr=1&trim=color&updated_at=1607060201&q=80"
							alt="" />
						<figcaption>
							<h4>Sneaker for Women</h4>
							<p>Luxury women rule the earth</p>
						</figcaption>
					</figure>
				</div>
				<div class="col-md-4 about-left">
					<figure class="effect-bubba">
						<img class="img-responsive"
							src="https://images.stockx.com/images/Air-Jordan-3-Retro-DJ-Khaled-Another-One.png?fit=fill&bg=FFFFFF&w=480&h=320&fm=webp&auto=compress&dpr=1&trim=color&updated_at=1608513989&q=80"
							alt="" />
						<figcaption>
							<h4>Smart Sneaker</h4>
							<p>Get in the technology race</p>
						</figcaption>
					</figure>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>
	<!--about-end-->
	<!--product-starts-->
	<div class="product">
		<div class="container">
			<div class="product-top">
				<div class="product-one">
					<% if(locals.products) { products.slice(-8).forEach((pd)=> { %>
						<div class="col-md-3 product-left">
							<div class="product-main simpleCart_shelfItem">
								<a href="/product/<%= pd._id %>" class="mask"><img class="img-responsive zoom-img"
										src="<%= pd.image %>" alt="" /></a>
								<div class="product-bottom">
									<h3>
										<%= pd.name %>
									</h3>
									<p>
										<%= pd.brand %>
									</p>
									<h4><a class="item_add" href="#"><i></i></a> <span class=" item_price">$ <%=
												pd.price %></span></h4>
								</div>
								<div class="srch">
									<span>-50%</span>
								</div>
							</div>
						</div>
						<% })} %>
				</div>
			</div>
		</div>
	</div>
	<!--product-end-->
	<!--information-starts-->
	<div class="information">
		<div class="container">
			<div class="infor-top">
				<div class="col-md-3 infor-left">
					<h3>Follow Us</h3>
					<ul>
						<li><a href="#"><span class="fb"></span>
								<h6>Facebook</h6>
							</a></li>
						<li><a href="#"><span class="twit"></span>
								<h6>Twitter</h6>
							</a></li>
						<li><a href="#"><span class="google"></span>
								<h6>Google+</h6>
							</a></li>
					</ul>
				</div>
				<div class="col-md-3 infor-left">
					<h3>Information</h3>
					<ul>
						<li><a href="#">
								<p>Specials</p>
							</a></li>
						<li><a href="#">
								<p>New Products</p>
							</a></li>
						<li><a href="#">
								<p>Our Stores</p>
							</a></li>
						<li><a href="contact.html">
								<p>Contact Us</p>
							</a></li>
						<li><a href="#">
								<p>Top Sellers</p>
							</a></li>
					</ul>
				</div>
				<div class="col-md-3 infor-left">
					<h3>My Account</h3>
					<ul>
						<li><a href="account">
								<p>My Account</p>
							</a></li>
						<li><a href="#">
								<p>My Credit slips</p>
							</a></li>
						<li><a href="#">
								<p>My Merchandise returns</p>
							</a></li>
						<li><a href="#">
